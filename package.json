{
  "name": "monofile-utilities",
  "version": "2.0.1",
  "description": "A utilities library for some misc functions.",
  "author": "acrazing <joking.young@gmail.com>",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "files": [
    "src/",
    "esm/",
    "cjs/"
  ],
  "sideEffects": [
    "*.css",
    "*.less",
    "*.scss"
  ],
  "scripts": {
    "clean": "rm -rf cjs esm build temp .cache",
    "build:esm": "tsc --outDir esm --module esnext --declaration --declarationMap",
    "build:cjs": "tsc --outDir cjs --module commonjs --declaration --declarationMap",
    "build": "npm run clean && run-p build:esm build:cjs",
    "tsnode": "ts-node -O '{\"resolveJsonModule\":true,\"module\":\"commonjs\",\"target\":\"es5\"}'",
    "prepublishOnly": "npm run build",
    "test": "echo 'nothing to do!'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acrazing/utilities.git"
  },
  "keywords": [
    "utilities",
    "form",
    "map",
    "cookie",
    "storage",
    "validate",
    "nonenumerable"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/acrazing/utilities/issues"
  },
  "homepage": "https://github.com/acrazing/utilities#readme",
  "dependencies": {
    "@types/lodash": "*",
    "@types/node": "*",
    "@types/yargs": "*",
    "lodash": "*",
    "tslib": "*",
    "yargs": "*"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "typescript": "*"
  },
  "peerDependencies": { }
}
